#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x559030fecea0 .scope module, "rc_tb" "rc_tb" 2 2;
 .timescale 0 0;
v0x559030fff780_0 .var "clk", 0 0;
v0x559030fff850_0 .net "counter", 3 0, v0x559030fff580_0;  1 drivers
v0x559030fff920_0 .var "reset", 0 0;
S_0x559030fed030 .scope module, "DUT" "rc" 2 9, 3 1 0, S_0x559030fecea0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /OUTPUT 4 "counter";
v0x559030fb4ba0_0 .net "clk", 0 0, v0x559030fff780_0;  1 drivers
v0x559030fff580_0 .var "counter", 3 0;
v0x559030fff660_0 .net "reset", 0 0, v0x559030fff920_0;  1 drivers
E_0x559030feb9e0 .event posedge, v0x559030fff660_0, v0x559030fb4ba0_0;
    .scope S_0x559030fed030;
T_0 ;
    %wait E_0x559030feb9e0;
    %load/vec4 v0x559030fff660_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 1, 0, 4;
    %assign/vec4 v0x559030fff580_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x559030fff580_0;
    %parti/s 3, 0, 2;
    %load/vec4 v0x559030fff580_0;
    %parti/s 1, 3, 3;
    %concat/vec4; draw_concat_vec4
    %assign/vec4 v0x559030fff580_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x559030fecea0;
T_1 ;
    %delay 5, 0;
    %load/vec4 v0x559030fff780_0;
    %inv;
    %store/vec4 v0x559030fff780_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x559030fecea0;
T_2 ;
    %vpi_call 2 20 "$dumpfile", "rc_test.vcd" {0 0 0};
    %vpi_call 2 21 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x559030fecea0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x559030fff780_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x559030fff920_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x559030fff920_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x559030fff920_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x559030fff920_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x559030fff920_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x559030fff920_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x559030fff920_0, 0, 1;
    %delay 155, 0;
    %vpi_call 2 30 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
