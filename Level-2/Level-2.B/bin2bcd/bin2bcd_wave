 .timescale 0 0;
    .port_info 0 /INPUT 8 "bin";
    .port_info 1 /OUTPUT 12 "bcd";
v0x5653961cb7b0_0 .var "bcd", 11 0;
v0x5653961cbba0_0 .net "bin", 7 0, v0x565396217400_0;  1 drivers
v0x565396216f30_0 .var "hundreds", 3 0;
v0x565396216ff0_0 .var "ones", 3 0;
v0x5653962170d0_0 .var "temp", 6 0;
v0x565396217200_0 .var "tens", 3 0;
E_0x565396202d50 .event edge, v0x5653961cbba0_0;
    .scope S_0x565396204890;
T_0 ;
    %wait E_0x565396202d50;
    %load/vec4 v0x5653961cbba0_0;
    %pad/u 32;
    %pushi/vec4 100, 0, 32;
    %div;
    %pad/u 4;
    %store/vec4 v0x565396216f30_0, 0, 4;
    %load/vec4 v0x5653961cbba0_0;
    %pad/u 32;
    %pushi/vec4 100, 0, 32;
    %mod;
    %pad/u 7;
    %store/vec4 v0x5653962170d0_0, 0, 7;
    %load/vec4 v0x5653962170d0_0;
    %pad/u 32;
    %pushi/vec4 10, 0, 32;
    %div;
    %pad/u 4;
    %store/vec4 v0x565396217200_0, 0, 4;
    %load/vec4 v0x5653962170d0_0;
    %pad/u 32;
    %pushi/vec4 10, 0, 32;
    %mod;
    %pad/u 4;
    %store/vec4 v0x565396216ff0_0, 0, 4;
    %load/vec4 v0x565396216f30_0;
    %load/vec4 v0x565396217200_0;
    %concat/vec4; draw_concat_vec4
    %load/vec4 v0x565396216ff0_0;
    %concat/vec4; draw_concat_vec4
    %store/vec4 v0x5653961cb7b0_0, 0, 12;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x565396204700;
T_1 ;
    %vpi_call 2 7 "$dumpfile", "bin2bcd_test.vcd" {0 0 0};
    %vpi_call 2 8 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x565396204700 {0 0 0};
    %pushi/vec4 12, 0, 8;
    %store/vec4 v0x565396217400_0, 0, 8;
    %delay 5, 0;
    %pushi/vec4 101, 0, 8;
    %store/vec4 v0x565396217400_0, 0, 8;
    %delay 5, 0;
    %pushi/vec4 111, 0, 8;
    %store/vec4 v0x565396217400_0, 0, 8;
    %delay 5, 0;
    %pushi/vec4 230, 0, 8;
    %store/vec4 v0x565396217400_0, 0, 8;
    %delay 5, 0;
    %pushi/vec4 180, 0, 8;
    %store/vec4 v0x565396217400_0, 0, 8;
    %delay 5, 0;
    %pushi/vec4 100, 0, 8;
    %store/vec4 v0x565396217400_0, 0, 8;
    %delay 5, 0;
    %pushi/vec4 106, 0, 8;
    %store/vec4 v0x565396217400_0, 0, 8;
