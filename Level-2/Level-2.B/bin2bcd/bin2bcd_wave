S_0x565396204700 .scope module, "bin2bcd_tb" "bin2bcd_tb" 2 1;
 .timescale 0 0;
v0x565396217340_0 .net "bcd", 11 0, v0x5653961cb7b0_0;  1 drivers
v0x565396217400_0 .var "bin", 7 0;
