    .scope S_0x55ac803330a0;
T_0 ;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x55ac802faba0_0, 0, 4;
    %end;
    .thread T_0;
    .scope S_0x55ac803330a0;
T_1 ;
    %wait E_0x55ac802f9710;
    %load/vec4 v0x55ac80348de0_0;
    %pad/u 32;
    %cmpi/e 1, 0, 32;
    %jmp/0xz  T_1.0, 4;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x55ac802faba0_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x55ac80348eb0_0;
    %pad/u 32;
    %cmpi/e 1, 0, 32;
    %jmp/0xz  T_1.2, 4;
    %load/vec4 v0x55ac802faba0_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x55ac802faba0_0, 0;
    %jmp T_1.3;
T_1.2 ;
    %load/vec4 v0x55ac802faba0_0;
    %subi 1, 0, 4;
    %assign/vec4 v0x55ac802faba0_0, 0;
T_1.3 ;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x55ac80332f10;
T_2 ;
    %delay 5, 0;
    %load/vec4 v0x55ac80349020_0;
    %inv;
    %store/vec4 v0x55ac80349020_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x55ac80332f10;
T_3 ;
    %vpi_call 2 11 "$dumpfile", "sync_test.vcd" {0 0 0};
