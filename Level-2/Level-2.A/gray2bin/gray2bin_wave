#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x563190edc000 .scope module, "gray2bin_tb" "gray2bin_tb" 2 1;
 .timescale 0 0;
v0x563190eee420_0 .net "b", 1 4, v0x563190ea37b0_0;  1 drivers
v0x563190eee4e0_0 .var "g", 1 4;
S_0x563190edc190 .scope module, "DUT" "gray2bin" 2 5, 3 1 0, S_0x563190edc000;
 .timescale 0 0;
    .port_info 0 /INPUT 4 "g";
    .port_info 1 /OUTPUT 4 "b";
v0x563190ea37b0_0 .var "b", 1 4;
v0x563190ea3ba0_0 .net "g", 1 4, v0x563190eee4e0_0;  1 drivers
E_0x563190ea2520 .event edge, v0x563190ea3ba0_0;
    .scope S_0x563190edc190;
T_0 ;
    %wait E_0x563190ea2520;
    %load/vec4 v0x563190ea3ba0_0;
    %parti/s 1, 3, 3;
    %ix/load 4, 3, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x563190ea37b0_0, 4, 1;
    %load/vec4 v0x563190ea37b0_0;
    %parti/s 1, 3, 3;
    %load/vec4 v0x563190ea3ba0_0;
    %parti/s 1, 2, 3;
    %xor;
    %ix/load 4, 2, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x563190ea37b0_0, 4, 1;
    %load/vec4 v0x563190ea37b0_0;
    %parti/s 1, 2, 3;
    %load/vec4 v0x563190ea3ba0_0;
    %parti/s 1, 1, 2;
    %xor;
    %ix/load 4, 1, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x563190ea37b0_0, 4, 1;
    %load/vec4 v0x563190ea37b0_0;
    %parti/s 1, 1, 2;
    %load/vec4 v0x563190ea3ba0_0;
    %parti/s 1, 0, 2;
    %xor;
    %ix/load 4, 0, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x563190ea37b0_0, 4, 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x563190edc000;
T_1 ;
    %vpi_call 2 7 "$dumpfile", "gray2bin_test.vcd" {0 0 0};
    %vpi_call 2 8 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x563190edc000 {0 0 0};
    %pushi/vec4 4, 0, 4;
    %store/vec4 v0x563190eee4e0_0, 0, 4;
    %delay 5, 0;
    %pushi/vec4 4, 0, 4;
    %store/vec4 v0x563190eee4e0_0, 0, 4;
    %delay 5, 0;
    %pushi/vec4 12, 0, 4;
    %store/vec4 v0x563190eee4e0_0, 0, 4;
    %delay 5, 0;
    %pushi/vec4 5, 0, 4;
    %store/vec4 v0x563190eee4e0_0, 0, 4;
    %delay 5, 0;
    %pushi/vec4 10, 0, 4;
    %store/vec4 v0x563190eee4e0_0, 0, 4;
    %delay 5, 0;
    %pushi/vec4 6, 0, 4;
    %store/vec4 v0x563190eee4e0_0, 0, 4;
    %delay 5, 0;
    %vpi_call 2 16 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "gray2bin_tb.v";
    "gray2bin.v";
