#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5620a89872b0 .scope module, "dec2to4_tb" "dec2to4_tb" 2 1;
 .timescale 0 0;
v0x5620a8999eb0_0 .var "E", 0 0;
v0x5620a8999f80_0 .var "W", 1 0;
v0x5620a899a050_0 .net "Y", 0 3, v0x5620a8999d70_0;  1 drivers
S_0x5620a8987440 .scope module, "DUT" "dec2to4" 2 5, 3 1 0, S_0x5620a89872b0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "E";
    .port_info 1 /INPUT 2 "W";
    .port_info 2 /OUTPUT 4 "Y";
v0x5620a894eba0_0 .net "E", 0 0, v0x5620a8999eb0_0;  1 drivers
v0x5620a8999c90_0 .net "W", 1 0, v0x5620a8999f80_0;  1 drivers
v0x5620a8999d70_0 .var "Y", 0 3;
E_0x5620a894d450 .event edge, v0x5620a8999c90_0, v0x5620a894eba0_0;
    .scope S_0x5620a8987440;
T_0 ;
    %wait E_0x5620a894d450;
    %load/vec4 v0x5620a894eba0_0;
    %pad/u 32;
    %cmpi/e 0, 0, 32;
    %jmp/0xz  T_0.0, 4;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x5620a8999d70_0, 0, 4;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5620a8999c90_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 2;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 2;
    %cmp/u;
    %jmp/1 T_0.3, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 2;
    %cmp/u;
    %jmp/1 T_0.4, 6;
    %dup/vec4;
    %pushi/vec4 3, 0, 2;
    %cmp/u;
    %jmp/1 T_0.5, 6;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x5620a8999d70_0, 0, 4;
    %jmp T_0.7;
T_0.2 ;
    %pushi/vec4 8, 0, 4;
    %store/vec4 v0x5620a8999d70_0, 0, 4;
    %jmp T_0.7;
T_0.3 ;
    %pushi/vec4 4, 0, 4;
    %store/vec4 v0x5620a8999d70_0, 0, 4;
    %jmp T_0.7;
T_0.4 ;
    %pushi/vec4 2, 0, 4;
    %store/vec4 v0x5620a8999d70_0, 0, 4;
    %jmp T_0.7;
T_0.5 ;
    %pushi/vec4 1, 0, 4;
    %store/vec4 v0x5620a8999d70_0, 0, 4;
    %jmp T_0.7;
T_0.7 ;
    %pop/vec4 1;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x5620a89872b0;
T_1 ;
    %vpi_call 2 7 "$dumpfile", "dec2to4_wave.vcd" {0 0 0};
    %vpi_call 2 8 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5620a89872b0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5620a8999eb0_0, 0, 1;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x5620a8999f80_0, 0, 2;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5620a8999eb0_0, 0, 1;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x5620a8999f80_0, 0, 2;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5620a8999eb0_0, 0, 1;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x5620a8999f80_0, 0, 2;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5620a8999eb0_0, 0, 1;
    %pushi/vec4 2, 0, 2;
    %store/vec4 v0x5620a8999f80_0, 0, 2;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5620a8999eb0_0, 0, 1;
    %pushi/vec4 3, 0, 2;
    %store/vec4 v0x5620a8999f80_0, 0, 2;
    %delay 5, 0;
    %vpi_call 2 15 "$finish" {0 0 0};
    %end;
    .thread T_1;
